  "scripts": {
    "static-watch-css": "npm run build-css && node-sass-chokidar static/lp/comic/sass/style.scss --include-path ./node_modules -o static/lp/comic/css --watch --recursive",
    "static-build-css": "node-sass-chokidar static/lp/comic/sass/style.scss --include-path ./node_modules -o static/lp/comic/css",
    "build-css": "node-sass-chokidar ./src/assets/css/style.scss --include-path ./node_modules -o src/assets/css/",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/assets/css/style.scss --include-path ./node_modules -o src/assets/css/ --watch --recursive",
    "start": "sh -ac '. ./.env.local; react-scripts start'",
    "build": "sh -ac '. ./.env.${REACT_APP_ENV}; npm run build-css; react-scripts build'",
    "build:local": "REACT_APP_ENV=local npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "immutable": "^3.8.2",
    "validator": "^9.0.0"
  }
